# Text Type
text_variable = "Hello, Python!"

# Numeric Types
int_variable = 42
float_variable = 3.14
complex_variable = 2 + 3j

# Sequence Types
list_variable = [1, 2, 3, 4, 5]
tuple_variable = (10, 20, 30)
range_variable = range(0, 10, 2)  # Creates a range from 0 to 8 with a step of 2

# Mapping Type
dict_variable = {'name': 'John', 'age': 25, 'city': 'New York'}

# Set Types
set_variable = {1, 2, 3, 4, 5}
frozenset_variable = frozenset({1, 2, 3, 4, 5})

# Boolean Type
bool_variable = True

# Binary Types
bytes_variable = b'binary data'
bytearray_variable = bytearray(b'editable binary')
memoryview_variable = memoryview(b'binary data')

# None Type
none_variable = None

# Printing the variables
print("Text Type:", text_variable)
print("Numeric Types:", int_variable, float_variable, complex_variable)
print("Sequence Types:", list_variable, tuple_variable, list(range_variable))
print("Mapping Type:", dict_variable)
print("Set Types:", set_variable, frozenset_variable)
print("Boolean Type:", bool_variable)
print("Binary Types:", bytes_variable, bytearray_variable, memoryview_variable)
print("None Type:", none_variable)
